<?xml version="1.0" encoding="UTF-8"?>

<cmds regex="(AT)?\+?[&amp;]?[A-Z]*=?\??[\+]*">
	<cmd name="httpCommand" prefix="AT+QHTTP" delimiter=",">
		<type name="DOWNLOAD_FILE_FROM_HTTP_SERVER" prefix="DL">
			<class name="TEST" prefix="=?"></class>
			<class name="WRITE" prefix="=">
				<order>
					<param name="fileName"></param>
					<param name="length" optional="true"></param>
					<param name="waitTime" optional="true">
						<min>1</min>
						<max>65536</max>
					</param>
				</order>
			</class>
		</type>
		<type name="SET_HTTP_SERVER_URL" prefix="URL">
			<class name="TEST" prefix="=?"></class>
			<class name="WRITE" prefix="=">
				<order>
					<param name="URLLength" optional="false">
						<min>1</min>
						<max>450</max>
					</param>
					<param name="inputTime" optional="true">
						<min>1</min>
						<max>65536</max>
					</param>
				</order>
			</class>
		</type>
	</cmd>
	<cmd name="fileCommand" prefix="AT+QF" delimiter=",">
		<type name="GET_STORAGE_DATE_SIZE" prefix="LDS">
			<class name="TEST" prefix="=?"></class>
			<class name="READ" prefix="?"></class>
			<class name="WRITE" prefix="=">
				<order>
					<param name="namePattern"></param>
				</order>
			</class>
			<class name="EXECUTION"></class>
		</type>
		<type name="UPLOAD_FILE_TO_STORAGE" prefix="UPL">
			<class name="TEST" prefix="=?"></class>
			<class name="WRITE" prefix="=">
				<order>
					<param name="fileName" optional="false"></param>
					<param name="fileSize" optional="true"></param>
					<param name="timeout" optional="true"></param>
					<param name="turnOnACTMode" optional="true">
						<true>1</true>
						<false>0</false>
					</param>
				</order>
			</class>
		</type>
	</cmd>
	<cmd name="mmsCommand" delimiter="," prefix="AT+QMM">
		<type name="WRITE_MMS_MESSAGE" prefix="SW">
			<class name="WRITE" prefix="="
				parser="com.rtrk.atcommand.parser.WriteMMSMessageParser"></class>
		</type>
	</cmd>
	<cmd name="generalCommand" delimiter=",">
		<type name="DISPLAY_PRODUCT_IDENTIFICATION_INFORMATION" prefix="ATI">
			<class name="EXECUTION"></class>
		</type>
	</cmd>
	<cmd name="smsCommand" prefix="AT+" delimiter=",">
		<type name="DELETE_SMS_MESSAGE" prefix="CMGD">
			<class name="WRITE" prefix="=">
				<order>
					<param name="index"
						parser="com.rtrk.atcommand.parser.DeleteSMSMessageIndexParser"></param>
					<param name="delFlag" optional="true"></param>
				</order>
			</class>
		</type>
		<type name="SELECT_SMS_MESSAGE_FORMAT" prefix="CMGF">
			<class name="WRITE" prefix="=">
				<order>
					<param name="messageFormat" optional="true" environment="true"></param>
				</order>
			</class>
		</type>
		<type name="SEND_SMS_MESSAGE" prefix="CMGS">
			<class name="WRITE" prefix="="
				parser="com.rtrk.atcommand.parser.SendSMSMessageParser"></class>
		</type>
		<type name="RESTORE_SMS_SETTINGS" prefix="CRES">
			<class name="WRITE" prefix="=" optional="true"
				parser="com.rtrk.atcommand.parser.RestoreSMSSettingsParser"></class>
		</type>
	</cmd>
	<cmd name="generalCommand">
		<type name="SET_ALL_PARAMETERS_TO_MANUFACTURER_DEFAULTS" prefix="AT&amp;F">
			<class name="EXECUTION">
				<order>
					<param name="default" optional="true"></param>
				</order>
			</class>
		</type>
	</cmd>
	<cmd name="callRelatedCommand">
		<type name="SWITCH_FROM_DATA_MODE_TO_COMMAND_MODE" prefix="+++">
			<class name="EXECUTION"></class>
		</type>
		<type name="MOBILE_ORIGINATED_CALL_TO_DIAL_A_NUMBER" prefix="ATD">
			<class name="EXECUTION"
				parser="com.rtrk.atcommand.parser.MobileOriginatedCallParser"></class>
		</type>
	</cmd>
	<cmd name="ftpCommand" prefix="AT+QFTP" delimiter=",">
		<type name="SET_SOME_CONFIGURABLE_PARAMETERS_FOR_THE_FTP_SERVICE"
			prefix="CFG">
			<class name="WRITE" prefix="="
				parser="com.rtrk.atcommand.parser.FTPCFGParser"></class>
		</type>
		<type
			name="LIST_CONTENTS_OF_DIRECTORY_OR_FILE_INFORMATION_ON_THE_FTP_SERVER"
			prefix="LIST">
			<class name="WRITE" prefix="=" optional="true"
				parser="com.rtrk.atcommand.parser.ListDirectoryContentParser"></class>
		</type>
		<type name="LIST_FILE_NAMES_IN_THE_GIVEN_DIRECTORY_ON_THE_FTP_SERVER"
			prefix="NLIST">
			<class name="WRITE" prefix="=" optional="true"
				parser="com.rtrk.atcommand.parser.ListFileNamesParser"></class>
		</type>
	</cmd>
	<cmd name="tcpipCommand" prefix="AT+Q">
		<type name="ENABLE_MULTIPLE_TCPIP_SESSION" prefix="IMUX">
			<class name="WRITE" prefix="=">
				<order>
					<param name="enableMultipleTCPIPSession" environment="true">
						<true>1</true>
						<false>0</false>
					</param>
				</order>
			</class>
		</type>
		<type name="SEND_DATA_THROUGH_TCP_OR_UDP_CONNECTION" prefix="ISEND">
			<class name="WRITE" prefix="="
				parser="com.rtrk.atcommand.parser.SendDataTCPUDPParser"></class>
		</type>
	</cmd>
</cmds>